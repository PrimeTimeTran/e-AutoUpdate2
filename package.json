{
  "name": "1on1-auto-update",
  "version": "0.0.1",
  "description": "Chieu dep trai",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron --debug .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder build "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chieuduong-bocasay/electron-1on1-auto-update.git"
  },
  "author": "Chieu Duong",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.2.4",
    "electron-builder": "^22.3.2"
  },
  "build": {
    "appId": "com.electron.app",
    "publish": [
      {
        "provider": "github",
        "owner": "chieuduong-bocasay",
        "repo": "electron-1on1-auto-update",
        "token": "311c42e68ee580b07d071794f0f5a3475cbafb69"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^4.0.4"
  },
  "bugs": {
    "url": "https://github.com/chieuduong-bocasay/electron-1on1-auto-update"
  },
  "homepage": "https://github.com/chieuduong-bocasay/electron-1on1-auto-update",
  "keywords": [
    "electron",
    "react",
    "redux",
    "scss",
    "material",
    "webpack",
    "hot",
    "reload"
  ]
}
